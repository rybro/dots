{
   "background": {
      "persistent": true,
      "scripts": [ "scripts/chromereload.js", "scripts/background.js" ]
   },
   "content_scripts": [ {
      "css": [ "styles/main.css", "styles/iosbutton.css" ],
      "js": [ "scripts/popup.js", "scripts/jquery.2.1.3-min.js", "scripts/underscore-min.js" ],
      "matches": [ "https://soundcloud.com/*" ],
      "run_at": "document_end"
   } ],
   "default_locale": "en",
   "description": "De-clutter your SoundCloud Stream",
   "icons": {
      "128": "images/soundshroud.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "SoundShroud",
   "page_action": {
      "default_icon": {
         "19": "images/soundshroud.png",
         "38": "images/soundshroud.png"
      },
      "default_popup": "popup.html",
      "default_title": "SoundShroud",
      "matches": [ "https://soundcloud.com/*" ]
   },
   "permissions": [ "storage", "https://script.google.com/macros/s/AKfycbyCUDY1dT3WtEhiqJ0t8k5AzCk2akxMPZkfQmYm2IoFz5-DZ1zv/exec" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2.0.1",
   "web_accessible_resources": [ "images/soundshroud.png", "images/facebook.png", "images/twitter.png" ]
}
