{
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "main.js" ],
      "matches": [ "http://*.twitch.tv/*", "http://*.justin.tv/*" ],
      "run_at": "document_start"
   } ],
   "description": "Enhances Twitch.TV, Justin.TV and other streaming sites with new features, bug fixes, and reduced clutter.",
   "icons": {
      "128": "icon.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "StreamBurner (Previously BetterJTV)",
   "permissions": [ "http://*.justin.tv/", "http://*.twitch.tv/" ],
   "short_name": "StreamBurner",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.1.6"
}
