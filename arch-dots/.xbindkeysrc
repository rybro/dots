 # program launcher
"dmenu_run -dim 0.5 -i -x 415 -y 330 -w 450 -h 20 -l 4 -fn 'Terminus' -nb '#201F1D' -nf '#eddec9' -sb '#0f5246' -sf '#eddec9'"
	m:0x8 + c:39

"amixer set Master toggle"
    m:0x0 + c:121
    XF86AudioMute 

"amixer set Master 5-"
   XF86AudioLowerVolume

"amixer set Master 5+"
   XF86AudioRaiseVolume

"thunar"
    m:0x8 + c:40
    Alt + d

"xterm"
    m:0x8 + c:38
    Alt + a

"chromium"
    m:0x8 + c:25
    Alt + w


